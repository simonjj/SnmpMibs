    INDEX     { juniMplsIfMinorIndex }
    ::= { juniMplsIfMinorTable 1 }

JuniMplsIfMinorEntry ::= SEQUENCE {
    juniMplsIfMinorIndex                    InterfaceIndex,
    juniMplsIfMinorRowStatus                RowStatus,
    juniMplsIfMinorLowerIfIndex             InterfaceIndexOrZero,
    juniMplsIfMinorAdminStatus              INTEGER,
