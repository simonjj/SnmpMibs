    usdMplsIfMajorLdpVpiStop        UsdMajorCfgVpiRanges,
    usdMplsIfMajorLdpVciStart       UsdMajorCfgVciRanges,
    usdMplsIfMajorLdpVciStop        UsdMajorCfgVciRanges,
    usdMplsIfMajorRsvpAdminStatus   INTEGER,
