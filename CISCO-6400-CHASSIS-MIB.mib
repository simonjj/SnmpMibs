	MODULE-IDENTITY,
        OBJECT-TYPE,
        NOTIFICATION-TYPE,
        Counter32,
        Integer32
		FROM SNMPv2-SMI

        TEXTUAL-CONVENTION,
	RowStatus,
        DisplayString
