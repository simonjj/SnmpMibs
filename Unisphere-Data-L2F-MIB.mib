    usdL2fDestStatIfIndex
        InterfaceIndex,
    usdL2fDestStatCtlRecvOctets
        Counter32,
    usdL2fDestStatCtlRecvPackets
        Counter32,
    usdL2fDestStatCtlRecvErrors
        Counter32,
    usdL2fDestStatCtlRecvDiscards
        Counter32,
    usdL2fDestStatCtlSendOctets
        Counter32,
    usdL2fDestStatCtlSendPackets
