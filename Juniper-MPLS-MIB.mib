    juniMplsIfMinorTunnelMetricMode         INTEGER,
    juniMplsIfMinorAbsoluteTunnelMetric     Unsigned32,
    juniMplsIfMinorRelativeTunnelMetric     Integer32,
    juniMplsIfMinorLabelDistProto           INTEGER,
    juniMplsIfMinorAnnounceToOspf           TruthValue,
    juniMplsIfMinorAnnounceToIsis           TruthValue,
