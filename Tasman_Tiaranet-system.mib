--
-- VLAN interface stats table. 
-- 
-- 

vlanInterfaceStatsTable 		OBJECT-TYPE
   	SYNTAX         	SEQUENCE OF VlanInterfaceStatsEntry
   	MAX-ACCESS     	not-accessible
    	STATUS         	current
   	DESCRIPTION  	"VLAN Interface statistics Table."
   	::= { vlanInterfaceGroup 1 }

vlanInterfaceStatsEntry 		OBJECT-TYPE
    	SYNTAX        	VlanInterfaceStatsEntry
    	MAX-ACCESS    	not-accessible
   	STATUS      	current
    	DESCRIPTION 	"A VLAN interface stats table entry."
    	INDEX         	{ vlanInterfaceIndex }
    	::= { vlanInterfaceStatsTable 1 }

VlanInterfaceStatsEntry ::= SEQUENCE {
	vlanInterfaceIndex    	INTEGER,
	vlanInterfaceName     	DisplayString,
	vlanTagID 	        INTEGER,
	vlanTaggedPkts 		INTEGER,
	vlanTaggedBytes      	DisplayString,
	vlanUnTaggedPkts 	INTEGER,
	vlanUnTaggedBytes      	DisplayString
			        }

vlanInterfaceIndex	OBJECT-TYPE
     	SYNTAX          INTEGER	
     	MAX-ACCESS    	not-accessible
    	STATUS        	current
     	DESCRIPTION   	"VLAN interface index for the table"
      ::= { vlanInterfaceStatsEntry 1 }

vlanInterfaceName           OBJECT-TYPE
    	SYNTAX        	DisplayString 
    	MAX-ACCESS    	read-only
    	STATUS        	current
     	DESCRIPTION   	"VLAN interface name."                  
     	::= { vlanInterfaceStatsEntry 2 }

vlanTagID            OBJECT-TYPE
     	SYNTAX          INTEGER	
     	MAX-ACCESS    	read-only
    	STATUS        	current
     	DESCRIPTION   	"VLAN tagging ID of the interface."
      ::= { vlanInterfaceStatsEntry 3 }

vlanTaggedPkts 			OBJECT-TYPE
     	SYNTAX          INTEGER	
     	MAX-ACCESS    	read-only
    	STATUS        	current
     	DESCRIPTION   	"Packets tagged on this VLAN interface."
      ::= { vlanInterfaceStatsEntry 4 }

vlanTaggedBytes             OBJECT-TYPE
    	SYNTAX        	DisplayString 
    	MAX-ACCESS    	read-only
    	STATUS        	current
     	DESCRIPTION   	"Bytes tagged on this VLAN interface."
     	::= { vlanInterfaceStatsEntry 5 }

vlanUnTaggedPkts 			OBJECT-TYPE
     	SYNTAX          INTEGER	
     	MAX-ACCESS    	read-only
    	STATUS        	current
     	DESCRIPTION   	"Packets un-tagged on this VLAN interface."
      ::= { vlanInterfaceStatsEntry 6 }

vlanUnTaggedBytes             OBJECT-TYPE
    	SYNTAX        	DisplayString 
    	MAX-ACCESS    	read-only
    	STATUS        	current
     	DESCRIPTION   	"Bytes un-tagged on this VLAN interface."
     	::= { vlanInterfaceStatsEntry 7 }


--
-- sntpGroup
--
sntpClientEnabled OBJECT-TYPE
	SYNTAX		SntpEnabled
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION	"Sntp Client Enabled Flag."
        DEFVAL          { not-enabled }
	::= { sntpGroup 1 }    
            
sntpServerAddr OBJECT-TYPE
        SYNTAX          DisplayString (SIZE (0..30))
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION
            "The IP address or host name of an SNTP/NTP server.
             It can be set only when set along with the
             sntpClientEnabled object.
            "
        ::= { sntpGroup 2 }

                       
sntpTimeout OBJECT-TYPE
        SYNTAX           INTEGER 
        MAX-ACCESS       read-create
        STATUS           current
        DESCRIPTION
            "The timeout value for the sntp client.
             It can be set only when set along with the
             sntpClientEnabled object.
            "
        DEFVAL           { 1024 }
        ::= { sntpGroup 3 }

--
-- sntp mib objects
--

enableSntpNotification OBJECT-TYPE
        SYNTAX          TruthValue
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
               "Indicates whether the system produces the
                sntpNotification. Default is yes.
                "
        DEFVAL         { true }
        ::= { sntpNotificationEnables 1 }

sntpEnableNotification TRAP-TYPE
        ENTERPRISE sntpNotifications
        VARIABLES { sntpServerAddr,
                    sntpTimeout 
                  }
        DESCRIPTION
                "
                A sntpNotification is sent when an sntp client
                is enabled.
                "
        ::= 1

sntpDisableNotification TRAP-TYPE
        ENTERPRISE sntpNotifications
        VARIABLES { sntpServerAddr,
                    sntpTimeout 
                  }
        DESCRIPTION
                "
                A sntpNotification is sent when an sntp client
                is disabled.
                "
        ::= 2

sntpSuccessNotification TRAP-TYPE
       ENTERPRISE sntpNotifications
       VARIABLES { sntpServerAddr,
                   sntpTimeout 
                 }
       DESCRIPTION
               "
               A sntpSuccessNotification is sent when an sntp client
               is successful in receiving time from the server.
               "
       ::= 3

sntpErrorNotification TRAP-TYPE
       ENTERPRISE sntpNotifications
       VARIABLES { sntpServerAddr,
                   sntpTimeout 
                 }
       DESCRIPTION
               "
               A sntpErrorNotification is sent when an sntp client
               has error in receiving time from the server.
               "
       ::= 4       


