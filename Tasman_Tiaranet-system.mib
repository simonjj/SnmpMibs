        IpAddress,
        Counter32,
        MODULE-IDENTITY
                FROM SNMPv2-SMI
        DisplayString,TruthValue
                FROM SNMPv2-TC
        TRAP-TYPE
                FROM RFC-1215
	tiaraMgmt
	        FROM TIARA-NETWORKS-SMI;

tiaraSystemMib MODULE-IDENTITY 
  	LAST-UPDATED "0008180000Z"
  	ORGANIZATION "Tiara Networks, Inc."
  	CONTACT-INFO
                "       Tiara Networks
                        Customer Support

                		525 Race Street, Suite 100,
                        San Jose, CA 95126
                        USA

                  	Tel: +1 408-216-4700
                   	Fax: +1 408-216-4701

                		Email: support@tiaranetworks.com"

  	DESCRIPTION
  		"System group for Tiara Networks products."
  	REVISION      "0008180000Z"
  	DESCRIPTION
  		"Initial version of this MIB module."
        ::= { tiaraMgmt 1}

SntpEnabled ::= TEXTUAL-CONVENTION
