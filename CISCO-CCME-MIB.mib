    MANDATORY-GROUPS {  ccmeConfigGroup,
                        ccmeActiveStatsGroup,
                        ccmeHistoryStatsGroup,
                        ccmeNotifGroup
                     }
         ::= { ciscoCcmeMIBCompliances 1 }

