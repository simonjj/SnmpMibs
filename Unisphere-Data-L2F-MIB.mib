    usdL2fDestStatCtlSendDiscards
        Counter32,
    usdL2fDestStatPayRecvOctets
        Counter32,
    usdL2fDestStatPayRecvPackets
        Counter32,
    usdL2fDestStatPayRecvErrors
        Counter32,
    usdL2fDestStatPayRecvDiscards
        Counter32,
    usdL2fDestStatPaySendOctets
        Counter32,
    usdL2fDestStatPaySendPackets
        Counter32,
    usdL2fDestStatPaySendErrors
        Counter32,
    usdL2fDestStatPaySendDiscards
        Counter32
    }

usdL2fDestStatIfIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Same value as ifIndex for the corresponding entry in Interfaces MIB
        ifTable."
