        ::= { cspTcpCounters 12 }

    cspTcByteRcvSeq OBJECT-TYPE
        SYNTAX     Counter32
        UNITS      "bytes"
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION
                "The total amount of data received in sequence."
        ::= { cspTcpCounters 13 }

    --
