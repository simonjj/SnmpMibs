        DESCRIPTION
                "The total number of TCP packets sent."
        ::= { cspTcpCounters 8 }

    cspTcDataPktSent OBJECT-TYPE
        SYNTAX     Counter32
        UNITS      "number of packets"
        MAX-ACCESS read-only
