   atmIfVptPnniSigVcdTrafficDescType
      INTEGER,
   atmIfVptPnniSigVcdAtmServiceCategory
      INTEGER,
   atmIfVptPnniSigVcdQosClass
      INTEGER,
   atmIfVptPnniSigVcdTrafficShaping
