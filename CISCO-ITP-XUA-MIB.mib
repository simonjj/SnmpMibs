                 Gateway Mate."
        INDEX   {
                cItpXuaSgmName,         -- SGM Name, shared index
                cItpXuaSgmAddrNum       -- IP Address number
        }
        ::= { cItpXuaSgmRemoteIpTable  1 }

CItpXuaSgmRemoteIpTableEntry  ::= SEQUENCE {
        cItpXuaSgmAddrNum                   Unsigned32,
