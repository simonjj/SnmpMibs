usdMplsIfMinorHoldingPriority OBJECT-TYPE
    SYNTAX      UsdMinorCfgHoldingAndSetupPriorityRanges
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object allows configuration of the preemption holding priority for
        the tunnel before and after it has been established.  Lower the value
        higher the priority."
    ::= { usdMplsIfMinorEntry 18 }

usdMplsIfMinorTunnelName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(0..20))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object allows configuration of the tunnel name to be associated
        with tunnel settings."
    ::= { usdMplsIfMinorEntry 19 }

usdMplsIfMinorTunnelRetryTimes OBJECT-TYPE
    SYNTAX      UsdMinorCfgRetryTimesAndNorouteRanges
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object allows configuration of the number of retries for tunnel
        setup."
    DEFVAL    { 0 }
    ::= { usdMplsIfMinorEntry 20 }

usdMplsIfMinorTunnelRetryInterval OBJECT-TYPE
    SYNTAX      UsdMinorCfgRetryIntervalAndNorouteRanges
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object allows configuration of retry interval for tunnel setup."
    DEFVAL    { 5 }
    ::= { usdMplsIfMinorEntry 21 }

usdMplsIfMinorTunnelRetryTimesNoroute OBJECT-TYPE
    SYNTAX      UsdMinorCfgRetryTimesAndNorouteRanges
