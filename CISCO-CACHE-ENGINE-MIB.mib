        "Each entry contains information on a single event."
    INDEX          {cceEventsEntryIndex}
    ::= {cceEventsTable 1}

CceEventsEntry ::= SEQUENCE {
    cceEventsEntryIndex       Integer32,
    cceEventsEntryType        INTEGER,
    cceEventsEntryMessage     DisplayString,
    cceEventsEntryTime        DisplayString
}

cceEventsEntryIndex OBJECT-TYPE
    SYNTAX          Integer32 (0 | 1..999)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION
