   appnDlciBnnLsDefAdjacentCpName
      AsciiString,
   appnDlciBnnLsDefAdjacentCpType
      INTEGER,
   appnDlciBnnLsDefTgNum
      Unsigned32,
   appnDlciBnnLsDefDlusName
      AsciiString,
   appnDlciBnnLsDefBackupDlusName
      AsciiString,
   appnDlciBnnLsDefHprSupported
