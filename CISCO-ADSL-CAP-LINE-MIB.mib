CAdslAturCapPhysEntry ::= SEQUENCE {
    cAdslAturCapCurrRxGain          Gauge32
}

cAdslAturCapCurrRxGain OBJECT-TYPE
    SYNTAX      Gauge32
    UNITS       "tenth dB"
