    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table contains entries for IP interfaces present in the system."
    ::= { usdIpInterface 3 }

usdIpIfStatsEntry OBJECT-TYPE
    SYNTAX      UsdIpIfStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains statistics for an IP interface."
    INDEX     { usdIpIfStatsIndex }
    ::= { usdIpIfStatsTable 1 }

