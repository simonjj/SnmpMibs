   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This group contains all operational attributes for the path oriented 
      LCo."
::= { vsLCo 10 }

vsLCoPathDataEntry OBJECT-TYPE
   SYNTAX     VsLCoPathDataEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the vsLCoPathDataTable."
   INDEX {
      vsIndex,
      vsLCoIndex }
::= { vsLCoPathDataTable 1 }

VsLCoPathDataEntry ::= SEQUENCE {
   vsLCoState
      INTEGER,
   vsLCoOverrideRemoteName
      AsciiString,
   vsLCoEnd
      INTEGER,
   vsLCoCostMetric
      Unsigned32,
   vsLCoDelayMetric
      Unsigned32,
   vsLCoRoundTripDelay
      Unsigned32,
   vsLCoSetupPriority
      Unsigned32,
   vsLCoHoldingPriority
      Unsigned32,
   vsLCoRequiredTxBandwidth
      Gauge32,
   vsLCoRequiredRxBandwidth
      Gauge32,
   vsLCoRequiredTrafficType
      INTEGER,
   vsLCoPermittedTrunkTypes
      OCTET STRING,
   vsLCoRequiredSecurity
      Unsigned32,
   vsLCoRequiredCustomerParameter
      Unsigned32,
   vsLCoEmissionPriority
      Unsigned32,
   vsLCoDiscardPriority
      Unsigned32,
   vsLCoPathType
      INTEGER,
   vsLCoRetryCount
      Unsigned32,
   vsLCoPathFailureCount
