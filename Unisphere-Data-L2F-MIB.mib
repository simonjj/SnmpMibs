-- { usdL2fDestConfig 1 } reserved for "next ifIndex" acquisition

usdL2fDestConfigTable   OBJECT-TYPE
    SYNTAX      SEQUENCE OF UsdL2fDestConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The L2F destination configuration table.  Each entry in this table has
        a corresponding entry in the Interfaces MIB ifTable, and in
        usdL2fDestStatsTable."
    ::= { usdL2fDestConfig 2 }

usdL2fDestConfigEntry   OBJECT-TYPE
    SYNTAX      UsdL2fDestConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Describes the configuration attributes of an L2F destination."
    INDEX { usdL2fDestConfigIfIndex }
