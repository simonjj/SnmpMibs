   CiscoScsiLuEntry ::= SEQUENCE { 
     ciscoScsiLuIndex              ScsiIndexValue, 
     ciscoScsiLuDefaultLun         ScsiLUNOrZero, 
     ciscoScsiLuWwnName            ScsiNameIdOrZero, 
     ciscoScsiLuVendorId           SnmpAdminString, 
