        DESCRIPTION
                "The total number of TCP data packets sent."
        ::= { cspTcpCounters 9 }

    cspTcDataByteSent OBJECT-TYPE
        SYNTAX     Counter32
        UNITS      "bytes"
        MAX-ACCESS read-only
