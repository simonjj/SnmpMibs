usdMplsIfMajorLdpAdminStatus OBJECT-TYPE
    SYNTAX      INTEGER {
                    disable(0),
                    enable(1) }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object enables/disables LDP admin status on the MPLS interface."
    ::= { usdMplsIfMajorEntry 5 }

usdMplsIfMajorLdpProfileName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(0..20))
