    MODULE-IDENTITY,
    OBJECT-TYPE,
    Unsigned32,
    Integer32   	 FROM SNMPv2-SMI
    TruthValue,
    RowStatus     	 FROM SNMPv2-TC
    MODULE-COMPLIANCE,
    OBJECT-GROUP 	 FROM SNMPv2-CONF
