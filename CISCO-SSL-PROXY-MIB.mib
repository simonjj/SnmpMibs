        ::= { cspMIBGroups 12 }

    cspCpuStatusGroup OBJECT-GROUP
        OBJECTS  { 
            -- cspCpuStatusEntry 
            cspCpuStatus, cspCpuClearTime, 
            cspCpuProcessUtil, cspCpuInterruptUtil, 
            cspCpuProcessUtilIn5Sec, cspCpuProcessUtilIn1Min, 
