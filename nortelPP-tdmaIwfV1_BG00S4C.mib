-- recent call involved this protocol.

tdmaBcModem OBJECT IDENTIFIER ::= { tdmaBc 3 }



tdmaBcModemRowStatusTable OBJECT-TYPE
   SYNTAX     SEQUENCE OF TdmaBcModemRowStatusEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This entry controls the addition and deletion of 
      tdmaBcModem components."
::= { tdmaBcModem 1 }

tdmaBcModemRowStatusEntry OBJECT-TYPE
   SYNTAX     TdmaBcModemRowStatusEntry
   ACCESS     not-accessible
