::= { tdmaCsServProvEntry 8 }

tdmaCsSupportedService OBJECT-TYPE
   SYNTAX     INTEGER  {
                 asyncDataOnly(0),
                 g3FaxOnly(1),
                 analogFaxOnly(2),
                 packetOnly(3),
