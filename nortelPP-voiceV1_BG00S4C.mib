VsPlcRowStatusEntry ::= SEQUENCE {
   vsPlcRowStatus
      RowStatus,
   vsPlcComponentName
      DisplayString,
   vsPlcStorageType
      StorageType,
   vsPlcIndex
      NonReplicated }

