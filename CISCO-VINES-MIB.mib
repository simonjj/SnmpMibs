cvIfConfigLineup OBJECT-TYPE
	SYNTAX     TruthValue
	MAX-ACCESS read-only
	STATUS     current
	DESCRIPTION
		"VINES protocol line up/down."
	::= { cvIfConfigEntry 9 }

cvIfConfigFastokay OBJECT-TYPE
	SYNTAX     TruthValue
	MAX-ACCESS read-only
	STATUS     current
	DESCRIPTION
		"VINES protocol fast switching supported."
