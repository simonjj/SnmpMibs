    juniMplsIfMajorLdpVpiStop        JuniMajorCfgVpiRanges,
    juniMplsIfMajorLdpVciStart       JuniMajorCfgVciRanges,
    juniMplsIfMajorLdpVciStop        JuniMajorCfgVciRanges,
    juniMplsIfMajorRsvpAdminStatus   INTEGER,
    juniMplsIfMajorRsvpProfileName   DisplayString,
