frUniDlciVcFastSelectCall OBJECT-TYPE
   SYNTAX     INTEGER  {
                 no(0),
                 yes(1) }
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This attribute displays that this is a fast select call."
::= { frUniDlciVcCadEntry 13 }

frUniDlciVcPathReliability OBJECT-TYPE
   SYNTAX     INTEGER  {
                 high(0),
