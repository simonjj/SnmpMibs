    usdIpIfStatsOutForwardedOctets          Counter64,
    usdIpIfStatsOutSchedDropPackets         Counter64,
    usdIpIfStatsOutSchedDropOctets          Counter64,
    usdIpIfStatsOutRequestedPackets         Counter64,
    usdIpIfStatsOutRequestedOctets          Counter64,
    usdIpIfStatsOutPoliciedPackets          Counter64,
    usdIpIfStatsOutPoliciedOctets           Counter64,
    usdIpIfStatsGreenOutSchedDropPackets    Counter64,
    usdIpIfStatsYellowOutSchedDropPackets   Counter64,
