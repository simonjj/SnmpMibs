::= { vsLCoRowStatusEntry 2 }

vsLCoStorageType OBJECT-TYPE
   SYNTAX     StorageType 
   ACCESS     read-only
   STATUS     mandatory
   DESCRIPTION
     "This variable represents the storage type value for the
      vsLCo tables."
::= { vsLCoRowStatusEntry 4 }

vsLCoIndex OBJECT-TYPE
   SYNTAX     NonReplicated 
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "This variable represents the index for the vsLCo tables."
::= { vsLCoRowStatusEntry 10 }

vsLCoPathDataTable OBJECT-TYPE
