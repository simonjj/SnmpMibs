    juniMplsIfMinorTunnelName               DisplayString,
    juniMplsIfMinorTunnelRetryTimes
        JuniMinorCfgRetryTimesAndNorouteRanges,
    juniMplsIfMinorTunnelRetryInterval
        JuniMinorCfgRetryIntervalAndNorouteRanges,
    juniMplsIfMinorTunnelRetryTimesNoroute
        JuniMinorCfgRetryTimesAndNorouteRanges,
    juniMplsIfMinorTunnelRetryIntervalNoroute
        JuniMinorCfgRetryIntervalAndNorouteRanges,
