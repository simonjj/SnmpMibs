   SYNTAX     TimeOperEntry
   ACCESS     not-accessible
   STATUS     mandatory
   DESCRIPTION
     "An entry in the timeOperTable."
   INDEX {
      timeIndex }
::= { timeOperTable 1 }

TimeOperEntry ::= SEQUENCE {
   timeNetworkTime
      EnterpriseDateAndTime,
   timeSyncStatus
      INTEGER,
