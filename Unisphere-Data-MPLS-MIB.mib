        To create an entry in this table, the following entry objects MUST be
        explicitly configured:
        usdMplsTunnelProfileTunnelEndpointRowStatus."
     ::= { usdMplsTunnelProfileTunnelEndpointEntry 4 }



-- //////////////////////////////////////////////////////////////////////
--
-- Explicit Path Configurations
--
-- //////////////////////////////////////////////////////////////////////

--
-- Explicit Path Table
--
usdMplsExplicitPathTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF UsdMplsExplicitPathEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Table is a list of hops configurations for explicit paths."
    ::= { usdMplsExplicitPathGroup 1 }

usdMplsExplicitPathEntry OBJECT-TYPE
    SYNTAX      UsdMplsExplicitPathEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry represents one hop configuration for the particular
        explicit path."
    INDEX     { usdMplsExplicitPathName }
    ::= { usdMplsExplicitPathTable 1 }

UsdMplsExplicitPathEntry ::= SEQUENCE {
    usdMplsExplicitPathName         DisplayString,
    usdMplsExplicitPathAdminState   INTEGER,
    usdMplsExplicitPathRowStatus    RowStatus }

usdMplsExplicitPathName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(0..20))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Configure the name for the explicit path used as table index."
    ::= { usdMplsExplicitPathEntry 1 }

usdMplsExplicitPathAdminState OBJECT-TYPE
    SYNTAX      INTEGER {
                    disable(0),
                    enable(1) }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Enable/disable explicit path adminstatus."
    DEFVAL    { disable }
    ::= { usdMplsExplicitPathEntry 2 }

usdMplsExplicitPathRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Creation/deletion of the explicit path is done through setting this
        object."
    ::= { usdMplsExplicitPathEntry 3 }

--
-- Explicit Path node Table
--
usdMplsExplicitPathNodeTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF UsdMplsExplicitPathNodeEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Table is a list of configured explicit paths."
    ::= { usdMplsExplicitPathGroup 2 }

usdMplsExplicitPathNodeEntry OBJECT-TYPE
    SYNTAX      UsdMplsExplicitPathNodeEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry represents one hop in a particular explicit path."
    INDEX     { usdMplsExplicitPathNodeName,
                usdMplsExplicitPathNodeIndexNumber }
    ::= { usdMplsExplicitPathNodeTable 1 }

UsdMplsExplicitPathNodeEntry ::= SEQUENCE {
    usdMplsExplicitPathNodeName             DisplayString,
    usdMplsExplicitPathNodeIndexNumber      Integer32,
    usdMplsExplicitPathNodeLoose            TruthValue,
    usdMplsExplicitPathNodeHopAddress       IpAddress,
    usdMplsExplicitPathNodeHopAddressMask   IpAddress,
    usdMplsExplicitPathNodeRowStatus        RowStatus }

usdMplsExplicitPathNodeName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(0..20))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Configure the name for the explicit path used in the table index."
    ::= { usdMplsExplicitPathNodeEntry 1 }

usdMplsExplicitPathNodeIndexNumber OBJECT-TYPE
    SYNTAX      Integer32 (1..255)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each explicit path hop will have an index associated with it.
        Configure each path hop index using this object."
    ::= { usdMplsExplicitPathNodeEntry 2 }

usdMplsExplicitPathNodeLoose OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Configure the explicit path hop as strict or loose.  Set along with
        HopAddress."
    DEFVAL    { false }
    ::= { usdMplsExplicitPathNodeEntry 3 }

usdMplsExplicitPathNodeHopAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Configure the hop IP address to setup explicit path.  Explicit path
        next hop will be created when the mentioned hop IP address doesn't
        belong to any of the existing hop even without setting the rowStatus
        createAndGo value.  The rowStatus value is used to destroy the node."
    ::= { usdMplsExplicitPathNodeEntry 4 }

usdMplsExplicitPathNodeHopAddressMask OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Configure the hop IP address mask.  Set along with hop address."
    DEFVAL    { 'ffffffff'H }
    ::= { usdMplsExplicitPathNodeEntry 5 }

usdMplsExplicitPathNodeRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Use this object for deletion of the node.  For node creation, this
        value have no effect.  Setting the node with IP address create the node.
        To delete the node set only this object."
    ::= { usdMplsExplicitPathNodeEntry 6 }


