IBM-SYSTEM-STORAGE-MIB DEFINITIONS ::= BEGIN

IMPORTS
   OBJECT-TYPE
        FROM RFC-1212

   TRAP-TYPE
        FROM RFC-1215

    DateAndTime, TruthValue
            FROM SNMPv2-TC

   ibmStorage,
   Boolean, Uint8, Sint8, Uint16, Sint16, Uint32,
   Sint32, Uint64, Sint64, Real32, Real64, String, Datetime
      FROM IBM-SYSTEM-MIB;

ibmStorageTrap       OBJECT IDENTIFIER ::= { ibmStorage 1 }
ibmStorageTrapInfo   OBJECT IDENTIFIER ::= { ibmStorage 2 }

--
-- ibmStorageController
--
ibmStorageControllerTable OBJECT-TYPE
   SYNTAX         SEQUENCE OF IbmStorageControllerEntry
   ACCESS         not-accessible
   STATUS         mandatory
   DESCRIPTION    "This table describes the asset information of the
                   Storage Controller objects"
   REFERENCE      "CIM_CARD"
::= { ibmStorage 3 }

ibmStorageControllerEntry OBJECT-TYPE
   SYNTAX         IbmStorageControllerEntry
   ACCESS         not-accessible
   STATUS         mandatory
   DESCRIPTION    "Each entry represents a unique Storage
                   Controller in the system"
   INDEX          { ibmStorageControllerKeyIndex }
::= { ibmStorageControllerTable 1 }

IbmStorageControllerEntry ::= SEQUENCE
{
   ibmStorageControllerKeyIndex String,     
   ibmStorageControllerModel String,
   ibmStorageControllerSerialNumber String
}

ibmStorageControllerKeyIndex OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "The index used by this table"
   REFERENCE      "CIM_CARD.KeyIndex"
::= { ibmStorageControllerEntry 1 }

ibmStorageControllerModel OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "The model of the controller"
   REFERENCE      "CIM_CARD.Model"
::= { ibmStorageControllerEntry 2 }

ibmStorageControllerSerialNumber OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "The serial number of the controller"
   REFERENCE      "CIM_CARD.SerialNumber"
::= { ibmStorageControllerEntry 3 }


--
-- ibmStorageDiskDrive
--
ibmStorageDiskDriveTable OBJECT-TYPE
   SYNTAX         SEQUENCE OF IbmStorageDiskDriveEntry
   ACCESS         not-accessible
   STATUS         mandatory
   DESCRIPTION    "This table describes the physical disk drives
                   attached to the storage controller(s)"
   REFERENCE      "CIM_DiskDrive"
::= { ibmStorage 4 }

ibmStorageDiskDriveTableUp OBJECT-TYPE
   SYNTAX String
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION "Returns Agent Version if Storage DiskDrive data is available."
   ::= { ibmStorageDiskDriveTable 0 }

ibmStorageDiskDriveEntry OBJECT-TYPE
   SYNTAX         IbmStorageDiskDriveEntry
   ACCESS         not-accessible
   STATUS         mandatory
   DESCRIPTION    "Each entry represents a single physcial disk drive in the 
                   system." 
   INDEX          { ibmStorageDiskDriveKeyIndex }
::= { ibmStorageDiskDriveTable 1 }

IbmStorageDiskDriveEntry ::= SEQUENCE
{
   ibmStorageDiskDriveKeyIndex String,
   ibmStorageDiskDriveDeviceID String,
   ibmStorageDiskDriveName String,
   ibmStorageDiskDriveElementName String,
   ibmStorageDiskDriveUnitsUsed String,
   ibmStorageDiskDriveOperationalStatus String
}

ibmStorageDiskDriveKeyIndex OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "The index used by this table"
   REFERENCE      "CIM_DiskDrive.KeyIndex"
::= { ibmStorageDiskDriveEntry 1 }

ibmStorageDiskDriveDeviceID OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "The DeviceID of the physical disk"
   REFERENCE      "CIM_DiskDrive.DeviceID"
::= { ibmStorageDiskDriveEntry 2 }

ibmStorageDiskDriveName OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "A user friendly name assigned to the physical disk"
   REFERENCE      "CIM_DiskDrive.Name"
::= { ibmStorageDiskDriveEntry 3 }

ibmStorageDiskDriveElementName OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "A user friendly name assigned to the physical disk"
   REFERENCE      "CIM_DiskDrive.ElementName"
::= { ibmStorageDiskDriveEntry 4 }

ibmStorageDiskDriveUnitsUsed OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "Number of Units used on the physcial disk"
   REFERENCE      "CIM_DiskDrive.UnitsUsed"
::= { ibmStorageDiskDriveEntry 5 }

ibmStorageDiskDriveOperationalStatus OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "The Operational Status of the physical disk.
                   Possible values are:

                   OK                 = Drive is online 
                   Error              = Drive is not functional
                   Stopped            = Drive is disabled
                   Stopping           = Drive is being disabled
                   Starting           = Drive is becoming enabled
                   Predictive Failure = Drive is functioning but predicting failure in near future"
   REFERENCE      "CIM_DiskDrive.OperationalStatus"
::= { ibmStorageDiskDriveEntry 6 }


--
-- ibmStorageStorageVolume
--
ibmStorageStorageVolumeTable OBJECT-TYPE
   SYNTAX         SEQUENCE OF IbmStorageStorageVolumeEntry
   ACCESS         not-accessible
   STATUS         mandatory
   DESCRIPTION    "This table represents a storage volume which can  
                   consist of one or more physical disk drives"
   REFERENCE      "CIM_StorageVolume"
::= { ibmStorage 5 }

ibmStorageStorageVolumeTableUp OBJECT-TYPE
   SYNTAX String
   ACCESS read-only
   STATUS mandatory
   DESCRIPTION "Returns Agent Version if Storage Volume data is available."
   ::= { ibmStorageStorageVolumeTable 0 }

ibmStorageStorageVolumeEntry OBJECT-TYPE
   SYNTAX         IbmStorageStorageVolumeEntry
   ACCESS         not-accessible
   STATUS         mandatory
   DESCRIPTION    "Each entry defines a single storage volume"
   INDEX          { ibmStorageStorageVolumeKeyIndex }
::= { ibmStorageStorageVolumeTable 1 }

IbmStorageStorageVolumeEntry ::= SEQUENCE
{
   ibmStorageStorageVolumeKeyIndex String,
   ibmStorageStorageVolumeDeviceID String,
   ibmStorageStorageVolumeName String,
   ibmStorageStorageVolumeElementName String,
   ibmStorageStorageVolumeBlockSize String,
   ibmStorageStorageVolumeConsumableBlocks String,
   ibmStorageStorageVolumeDataRedundancy Uint16,
   ibmStorageStorageVolumeNoSinglePointOfFailure Boolean,
   ibmStorageStorageVolumeOperationalStatus String
}

ibmStorageStorageVolumeKeyIndex OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "The index used by this table"
   REFERENCE      "CIM_StorageVolume.KeyIndex"
::= { ibmStorageStorageVolumeEntry 1 }

ibmStorageStorageVolumeDeviceID OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "The DeviceID of the storage volume"
   REFERENCE      "CIM_StorageVolume.DeviceID"
::= { ibmStorageStorageVolumeEntry 2 }

ibmStorageStorageVolumeName OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "A user friendly name assigned to the storage volume"
   REFERENCE      "CIM_StorageVolume.Name"
::= { ibmStorageStorageVolumeEntry 3 }

ibmStorageStorageVolumeElementName OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "A user friendly name assigned to the storage volume"
   REFERENCE      "CIM_StorageVolume.ElementName"
::= { ibmStorageStorageVolumeEntry 4 }

ibmStorageStorageVolumeBlockSize OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "The block size used by the storage volume"
   REFERENCE      "CIM_StorageVolume.BlockSize"
::= { ibmStorageStorageVolumeEntry 5 }

ibmStorageStorageVolumeConsumableBlocks OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "The number of consumable blocks in the storage volume"
   REFERENCE      "CIM_StorageVolume.ConsumableBlocks"
::= { ibmStorageStorageVolumeEntry 6 }

ibmStorageStorageVolumeDataRedundancy OBJECT-TYPE
   SYNTAX         Uint16
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "Number of complete copies of the data 
                   currently maintained"
   REFERENCE      "CIM_StorageVolume.DataRedundancy"
::= { ibmStorageStorageVolumeEntry 7 }

ibmStorageStorageVolumeNoSinglePointOfFailure OBJECT-TYPE
   SYNTAX         Boolean
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "Indicates if there is no single point of failure"
   REFERENCE      "CIM_StorageVolume.NoSinglePointOfFailure"
::= { ibmStorageStorageVolumeEntry 8 }

ibmStorageStorageVolumeOperationalStatus OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "Operations Status of the storage volume
                   Possible values are:
                   
                   Degraded = A drive has failed, a rebuild
                   is in process, or a spare drive is in use.

                   Error = The volume has an error, is broken,
                   or there has been data loss."
   REFERENCE      "CIM_StorageVolume.OperationalStatus"
::= { ibmStorageStorageVolumeEntry 9 }


--
-- ibmStorageSoftwareIdentity
--
ibmStorageSoftwareIdentityTable OBJECT-TYPE
   SYNTAX         SEQUENCE OF IbmStorageSoftwareIdentityEntry
   ACCESS         not-accessible
   STATUS         mandatory
   DESCRIPTION    "The table describes the Firmware, BIOS, and driver
                   information for physical components of the storage system."
   REFERENCE      "CIM_SoftwareIdentity"
::= { ibmStorage 6 }

ibmStorageSoftwareIdentityEntry OBJECT-TYPE
   SYNTAX         IbmStorageSoftwareIdentityEntry
   ACCESS         not-accessible
   STATUS         mandatory
   DESCRIPTION    "Each entry represents a unique software identity"
   INDEX          { ibmStorageSoftwareIdentityKeyIndex }
::= { ibmStorageSoftwareIdentityTable 1 }

IbmStorageSoftwareIdentityEntry ::= SEQUENCE
{
   ibmStorageSoftwareIdentityKeyIndex String,
   ibmStorageSoftwareIdentityDescription String,
   ibmStorageSoftwareIdentityCaption String,
   ibmStorageSoftwareIdentityElementName String,
   ibmStorageSoftwareIdentityInstanceID String,
   ibmStorageSoftwareIdentityManufacturer String,
   ibmStorageSoftwareIdentityVersionString String,
   ibmStorageSoftwareIdentitySerialNumber String
}

ibmStorageSoftwareIdentityKeyIndex OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "The index used by this table"
   REFERENCE      "CIM_SoftwareIdentity.KeyIndex"
::= { ibmStorageSoftwareIdentityEntry 1 }

ibmStorageSoftwareIdentityDescription OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "A description of the software item"
   REFERENCE      "CIM_SoftwareIdentity.Description"
::= { ibmStorageSoftwareIdentityEntry 2 }

ibmStorageSoftwareIdentityCaption OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "A user friendly name for the software item"
   REFERENCE      "CIM_SoftwareIdentity.Caption"
::= { ibmStorageSoftwareIdentityEntry 3 }

ibmStorageSoftwareIdentityElementName OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "A user friendly name for the software item"
   REFERENCE      "CIM_SoftwareIdentity.ElementName"
::= { ibmStorageSoftwareIdentityEntry 4 }

ibmStorageSoftwareIdentityInstanceID OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "An instanceID assigned to the software item"
   REFERENCE      "CIM_SoftwareIdentity.InstanceID"
::= { ibmStorageSoftwareIdentityEntry 5 }

ibmStorageSoftwareIdentityManufacturer OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "The manufacturer of the software"
   REFERENCE      "CIM_SoftwareIdentity.Manufacturer"
::= { ibmStorageSoftwareIdentityEntry 6 }

ibmStorageSoftwareIdentityVersionString OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "The current version of the software item"
   REFERENCE      "CIM_SoftwareIdentity.VersionString"
::= { ibmStorageSoftwareIdentityEntry 7 }

ibmStorageSoftwareIdentitySerialNumber OBJECT-TYPE
   SYNTAX         String
   ACCESS         read-only
   STATUS         mandatory
   DESCRIPTION    "The serial number of the device"
   REFERENCE      "CIM_SoftwareIdentity.SerialNumber"
::= { ibmStorageSoftwareIdentityEntry 8 }


--
-- Trap variables
--
ibmStorageTrapProviderName OBJECT-TYPE
    SYNTAX String
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The CIM Provider Name"
    ::= { ibmStorageTrapInfo 1 }

ibmStorageTrapEventID OBJECT-TYPE
    SYNTAX String
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "A unique EventID that identifies the item reporting the issue"
    ::= { ibmStorageTrapInfo 2 }

ibmStorageTrapPerceivedSeverity OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "Severity of the event
         0=Normal / OK 
         1=Warning 
         2=Critical "
    ::= { ibmStorageTrapInfo 3 }

ibmStorageDescription OBJECT-TYPE
    SYNTAX String
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The event message"
    ::= { ibmStorageTrapInfo 4 }

ibmStorageTrapEventTime OBJECT-TYPE
    SYNTAX String
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "Time that the event occured"
    ::= { ibmStorageTrapInfo 5 }

