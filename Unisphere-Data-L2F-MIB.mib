--
-- Session-level Configuration
--

-- { usdL2fSessionConfig 1 } reserved for "next ifIndex" acquisition

usdL2fSessionConfigTable   OBJECT-TYPE
    SYNTAX      SEQUENCE OF UsdL2fSessionConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The L2F session configuration table.  Each entry in this table has a
        corresponding entry in the Interfaces MIB ifTable, and in
        usdL2fSessionStatsTable."
    ::= { usdL2fSessionConfig 2 }

usdL2fSessionConfigEntry   OBJECT-TYPE
    SYNTAX      UsdL2fSessionConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Describes the configuration attributes of an L2F session."
    INDEX { usdL2fSessionConfigIfIndex }
    ::= { usdL2fSessionConfigTable 1 }

UsdL2fSessionConfigEntry ::= SEQUENCE {
    usdL2fSessionConfigIfIndex
        InterfaceIndex,
    usdL2fSessionConfigRowStatus
        RowStatus,
    usdL2fSessionConfigAdminState
        UsdL2fAdminState
    }

usdL2fSessionConfigIfIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
