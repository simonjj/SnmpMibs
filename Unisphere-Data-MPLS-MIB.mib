usdMplsIfMajorAdminStatus OBJECT-TYPE
    SYNTAX      INTEGER {
                    disable(0),
                    enable(1) }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Use this object to create MPLS major interface stacked over any link
