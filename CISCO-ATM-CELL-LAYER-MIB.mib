            caclHighInHecErrDiscCells, caclHighInHecErrCorrectedCells,
            caclHighOutXmtCLP0Cells, caclHighOutXmtCLP1Cells,
            caclHighOutValidOAMCells, caclHighOutRcvCLP0Cells,
