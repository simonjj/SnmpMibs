	ifIndex
       		FROM IF-MIB

	MODULE-COMPLIANCE, OBJECT-GROUP
       		FROM SNMPv2-CONF

	ciscoExperiment
		FROM CISCO-SMI;

cisco6400ChassisMIB	MODULE-IDENTITY
	LAST-UPDATED	"9801080000Z"
	ORGANIZATION    "Cisco Systems, Inc."
	CONTACT-INFO
		"	Cisco Systems
			Customer Service

		Postal: 170 W Tasman Drive
                        San Jose, CA  95134
                        USA
 
                   Tel: +1 800 553-NETS
 
                E-mail: cs-atm@cisco.com"
	DESCRIPTION
		"6400 Chassis MIB."
	REVISION	"9808050000Z"
	DESCRIPTION
		"Update chassis alarm threshold setup range & main-cpu auto-sync
		 flags."
	REVISION	"9712100000Z"
	DESCRIPTION
		"Initial version of this MIB module."
