    UsdNextIfIndex
        FROM Unisphere-Data-TC;


usdFrameRelayMIB  MODULE-IDENTITY
    LAST-UPDATED "200009261730Z" -- 26-Sep-00 1:30 PM EDT
    ORGANIZATION "Unisphere Networks, Inc."
    CONTACT-INFO
        "       Unisphere Networks, Inc.
        Postal: 10 Technology Park Drive
                Westford MA  01886
                USA
        Tel:    +1 978 589 5800
        Email:  mib@UnisphereNetworks.com"
    DESCRIPTION
        "The Frame Relay MIB for the Unisphere Networks Inc. enterprise."
